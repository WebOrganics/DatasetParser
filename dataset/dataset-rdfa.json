{
 "select": {
	"from": "rdfa.html",
	"prefix": {
		"value": "http://xmlns.com/foaf/0.1/",
		"dc": "http://purl.org/dc/elements/1.1/"
	},
	"where": {
		"property~=title": { "label": "dc:title" },
		"typeof~=Person": { 
			"where": {
				"property~=name": {}, 
				"property~=nick": {},
				"rel~=mbox": { "type": "uri" },
				"rel~=weblog": { "type": "uri" }
				} 
			}
		}
	}
}
{
"select": {
	"from": "hcal.html",
	"prefix": {
		"value":"http://www.w3.org/2002/12/cal/ical#" 
	},
	"where": {
		"body": {
			"label": "Vcalendar",
			"where": {
				"h1": {
					"label": "prodid",
					"content": "-//weborganics.co.uk//dataset//hcal $Date: 2010/05/01 19:08:00 $ (BETA)//EN"
				},
				".vevent": {
					"label": "component",
					"type": "uri"
				}
			}
		},
		".vevent": {
		  "label": "Vevent", 
			"where": {
				".summary": {}, 
				".dtstamp": {}, 
				".uid": {},
				".dtstart": {"type": "http://www.w3.org/2001/XMLSchema#dateTime"},
				".dtend": {"type": "http://www.w3.org/2001/XMLSchema#dateTime"},
				".class": {},
				".category": {}
				}
			}
		}	
	}
}
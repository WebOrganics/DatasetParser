{
"query": {
	"base": "http://weborganics.co.uk/dataset-transformations/article.html",
	"vocab": {
		"value":"http://purl.org/rss/1.0/",
		"dc": "http://purl.org/dc/elements/1.1/",
		"media": "http://purl.org/microformat/hmedia/"
 
	},
	"item": {
		"article": { 
			"label": "channel",
			"item": {
				"h1": { "label": "title" },
				"h2": { "label": "description" },
				"rel~=index": { "label": "link", "type": "resourceplain" },
				"#main": {
					"label": "items",
					"about": false,
					"item": {
						"ol": {
							"label": "rdf:Seq",
							"about": false,
							"item": {
								"section": { "label": "rdf:li", "type": "resource", "multiple": true }
								}
							}
						}
					}
				}
			},
			"section": {
				"label" : "item",
				"item" : {
					"rel~=bookmark" : { "label": "link", "type": "resourceplain" },
					"rel~=enclosure" : { "label": "media:enclosure", "type": "resource" },
					"h3": { "label": "title" },
					".vcard": { "label": "dc:creator" },
					"time": { "label": "dc:date",  "type": "datetime"  },
					"div": { "label": "description", "type": "cdata" }
				}
			}
		}	
	}
}